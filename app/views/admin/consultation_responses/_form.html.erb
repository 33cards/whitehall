<%= form_for [:admin, edition], as: :edition do |form| %>
  <%= form.errors %>
  <%= render partial: 'standard_fields', locals: { form: form, edition: edition }%>
  <%= render partial: 'attachment_fields', locals: { form: form, edition: edition, parent_type: :edition }%>
  <%= form.hidden_field :consultation_id %>
  <%= form.save_or_cancel %>
<% end %>
