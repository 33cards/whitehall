<div class="inside_gov_home">
  <% page_title "Coming soon" %>
  <% page_class "inside_gov" %>
  <div class="block-1">
    <div class="inner-block">
      <section class="page-header">
        <h1 class="page_title">Coming soon</h1>
      </section>
    </div>
  </div>

  <div class="block-2">
    <div class="inner-block">
      <p class="introduction">
        This document will be published on<br />
        <%= render_datetime_microformat(@document, :scheduled_publication) { @document.scheduled_publication.to_date.to_s(:long_ordinal) + " at " + @document.scheduled_publication.to_s(:time) }%>
      </p>
    </div>
  </div>
</div>
